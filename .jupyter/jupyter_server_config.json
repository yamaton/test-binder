{
  "SingleUserNotebookApp": {
    "root_dir": "work",
    "contents_manager_class": "jupyterfs.metamanager.MetaManager",
    "jpserver_extensions": {
      "jupyterfs.extension": true
    }
  },
  "ServerApp": {
    "root_dir": "work",
    "contents_manager_class": "jupyterfs.metamanager.MetaManager",
    "jpserver_extensions": {
      "jupyterfs.extension": true
    }
  },
  "ContentsManager": {
    "allow_hidden": true
  },
  "FileContentsManager":{
    "always_delete_dir": true
  },
  "LanguageServerManager": {
    "language_servers": {
      "triton-lsp": {
        "version": 2,
        "argv": [
          "/srv/conda/envs/notebook/bin/node",
          "/srv/conda/envs/notebook/etc/jupyter/node_modules/.bin/triton-lsp",
          "--stdio"
        ],
        "languages": [
          "bash",
          "sh"
        ],
        "mime_types": [
          "text/x-sh",
          "application/x-sh"
        ],
        "display_name": "triton-lsp"
      }
    }
  }
}