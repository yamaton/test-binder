# Auto-generated with h2o

_STARFusion()
{
    local i=1 cmd cur word_list
    cur="${COMP_WORDS[COMP_CWORD]}"

    # take the last word that's NOT starting with -
    while [[ ( "$i" < "$COMP_CWORD" ) ]]; do
        local s="${COMP_WORDS[i]}"
        case "$s" in
          -*) ;;
          *)
            cmd="$s"
            ;;
        esac
        (( i++ ))
    done

    case "$cmd" in
      *)
          word_list="  --left_fq --right_fq --chimeric_junction -J --genome_lib_dir --CPU --output_dir -O --show_full_usage_info --STAR_max_mate_dist --STAR_SJDBoverhangMin --STAR_SortedByCoordinate --STAR_limitBAMsortRAM --STAR_PATH --run_STAR_only --STAR_onepass --STAR_peOverlapNbasesMin --STAR_peOverlapMMp --STAR_chimMultimapScoreRange --STAR_chimMultimapNmax --STAR_chimNonchimScoreDropMin --STAR_outSAMattrRGline --min_pct_MM_nonspecific -M --no_filter --outTmpDir --min_sum_frags --require_LDAS --max_promiscuity --min_pct_dom_promiscuity --aggregate_novel_junction_dist --min_novel_junction_support --min_spanning_frags_only --min_alt_pct_junction --min_FFPM --no_remove_dups --skip_EM --skip_FFPM --no_annotation_filter --no_RT_artifact_filter --no_single_fusion_per_breakpoint --examine_coding_effect --extract_fusion_reads --FusionInspector --denovo_reconstruct --misc_FI_opts --version --STAR_use_shared_memory --STAR_LoadAndExit --STAR_Remove --tmpdir --verbose_level --max_sensitivity --full_Monty"
          COMPREPLY=( $(compgen -W "${word_list}" -- "${cur}") )
          ;;
    esac

}

## -o bashdefault and -o default are fallback
complete -o bashdefault -o default -F _STARFusion STAR-Fusion
