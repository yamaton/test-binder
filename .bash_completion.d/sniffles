# Auto-generated with h2o

_sniffles()
{
    local i=1 cmd cur word_list
    cur="${COMP_WORDS[COMP_CWORD]}"

    # take the last word that's NOT starting with -
    while [[ ( "$i" < "$COMP_CWORD" ) ]]; do
        local s="${COMP_WORDS[i]}"
        case "$s" in
          -*) ;;
          *)
            cmd="$s"
            ;;
        esac
        (( i++ ))
    done

    case "$cmd" in
      *)
          word_list="  -h --help --version -i --input -v --vcf --snf --reference --tandem-repeats --non-germline --phase -t --threads --minsupport --minsupport-auto-mult --minsvlen --minsvlen-screen-ratio --mapq --no-qc --qc-output-all --qc-stdev --qc-stdev-abs-max --qc-strand --qc-coverage --long-ins-length --long-del-length --long-del-coverage --long-dup-length --long-dup-coverage --max-splits-kb --max-splits-base --min-alignment-length --phase-conflict-threshold --detect-large-ins --cluster-binsize --cluster-r --cluster-repeat-h --cluster-repeat-h-max --cluster-merge-pos --cluster-merge-len --cluster-merge-bnd --genotype-ploidy --genotype-error --sample-id --genotype-vcf --combine-high-confidence --combine-low-confidence --combine-low-confidence-abs --combine-null-min-coverage --combine-match --combine-match-max --combine-separate-intra --combine-output-filtered --output-rnames --no-consensus --no-sort --no-progress --quiet --max-del-seq-len --symbolic --allow-overwrite --combine-consensus"
          COMPREPLY=( $(compgen -W "${word_list}" -- "${cur}") )
          ;;
    esac

}

## -o bashdefault and -o default are fallback
complete -o bashdefault -o default -F _sniffles sniffles
