    local_nonpersistent_flags+=("--quiet-mode")
    local_nonpersistent_flags+=("-Q")
    flags+=("--range-max=")
    two_word_flags+=("--range-max")
    two_word_flags+=("-M")
    local_nonpersistent_flags+=("--range-max")
