    flags_completion=()

    flags+=("--bed")
    local_nonpersistent_flags+=("--bed")
    flags+=("--flanking-region")
    flags+=("-f")
    local_nonpersistent_flags+=("--flanking-region")
    local_nonpersistent_flags+=("-f")
    flags+=("--forward=")
    two_word_flags+=("--forward")
    two_word_flags+=("-F")
    local_nonpersistent_flags+=("--forward")
    local_nonpersistent_flags+=("--forward=")
    local_nonpersistent_flags+=("-F")
    flags+=("--immediate-output")
