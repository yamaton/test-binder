}

_salmon_quantmerge ()
{
    local cur word_list
    word_list=" -v --version -h --help --quants --names -c --column --genes --missing -o --output" 
    cur="${COMP_WORDS[COMP_CWORD]}"
    COMPREPLY=( $(compgen -W "${word_list}" -- "${cur}") )
}

