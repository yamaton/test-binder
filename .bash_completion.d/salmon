{
    local cur word_list
    word_list=" -l --libType -i --index -r --unmatedReads -1 --mates1 -2 --mates2 -v --version -h --help -o --output -j --rad --sketch -p --threads --tgMap --hash --dropseq --chromiumV3 --chromium --gemcode --citeseq --celseq --celseq2 --splitseqV1 --splitseqV2 --quartzseq2 --sciseq3 --whitelist --featureStart --featureLength --noQuant --numCellBootstraps --numCellGibbsSamples --forceCells --expectCells --mrna --rrna --keepCBFraction --read-geometry --bc-geometry --umi-geometry --end --umiLength --barcodeLength --noem --freqThreshold --umiEditDistance --dumpfq --dumpBfh --dumpArborescences --dumpUmiGraph --dumpCellEq --dumpFeatures --dumpMtx --lowRegionMinNumBarcodes --maxNumBarcodes" 
